{
  "name": "uimt/mitake-sdk-php",
  "description": "A Mitake SMS SDK for PHP (Unofficial)",
  "type": "library",
  "keywords": [
    "mitake",
    "api",
    "sms"
  ],
  "homepage": "https://github.com/uimt/mitake-sdk-php",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/mitake-sdk-php/mitake-sdk-php/issues"
  },
  "require": {
    "php": "^7.3|^8.0",
    "guzzlehttp/guzzle": "^7.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "slim/slim": "^4.8",
    "squizlabs/php_codesniffer": "^3.6",
    "symfony/console": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Mitake\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mitake\\Console\\": "console",
      "Mitake\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  }
}
